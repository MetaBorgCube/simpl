{
  let
    regex = [aa]
  in 
    ("aa" ? regex)
  ; "a" ? [(a|b)]
  ; "b" ? [(a|b)]
  ; "aa" ? [aa]
  ; "aabb" ? [aabb]
  ; "" ? [&]
  ; "aa" ? [aa&]
  ; "12345678" ? [1234(((56|567)|5678)|&)]
  ; "123456" ? [1234(((56|567)|5678)|&)]
  ; "1234567" ? [1234(((56|567)|5678)|&)]
  ; "12345678" ? [1234(56((&|7)|78)|&)]
  ; "123412341234" ? [(1234)*]
  ; "1111" ? [(0|(1(01*0)*1))*]
}
